{
  "name": "teamorbit-backend",
  "version": "1.0.0",
  "description": "Backend API for TeamOrbit application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r module-alias/register dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "tsc --noEmit --pretty",
    "migrate": "ts-node -r tsconfig-paths/register src/database/dbMigrate.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-all": "npm run type-check && npm run format:check"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "api",
    "teamorbit"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "umzug": "^3.8.2",
    "zod": "^4.1.11",
    "zod-validation-error": "^4.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@src": "dist"
  }
}
